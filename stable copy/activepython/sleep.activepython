import time,sys

if __name__ == "__main__": 
    warmup = int(sys.argv[1]) if len(sys.argv ) >1 else 5
    mainloop = int(sys.argv[2]) if len(sys.argv ) >2 else 10
    t = time.time()  
    print ('--++ '+ str(t)) 
    time.sleep(warmup)
    t = time.time()
    print ('++-- '+ str(t))
    time.sleep (mainloop)
